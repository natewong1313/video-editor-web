{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public --kv=USERS_KV",
    "lint": "eslint ./app",
    "lint:debug": "eslint ./app --debug",
    "lint:fix": "eslint ./app --fix"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.15.0",
    "@remix-run/cloudflare-pages": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.8",
    "lucide-react": "^0.176.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-validated-form": "^4.6.11",
    "tailwind-merge": "^1.12.0",
    "tailwindcss-animate": "^1.0.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "wrangler": "^2.15.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
